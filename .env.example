# Vite Client Environment Variables for Appwrite
# Copy this file to `.env` and fill in the values. Vite exposes variables prefixed with VITE_.

# Appwrite endpoint (keep default unless self-hosting)
VITE_APPWRITE_ENDPOINT=https://cloud.appwrite.io/v1

# REQUIRED: Appwrite Project ID
VITE_APPWRITE_PROJECT_ID=

# Optional: If you use Appwrite Database and Storage in this app
VITE_APPWRITE_DATABASE_ID=culina_db
VITE_APPWRITE_COLLECTION_ID_POSTS=posts
VITE_APPWRITE_COLLECTION_ID_PROFILES=profiles
VITE_APPWRITE_BUCKET_ID_IMAGES=images
